#math.h
snippet abs
abbr int abs(int n)
  abs(${1:n})

snippet pow
abbr double pow(double base,double exp)
  pow(${1:base},${2:exp})

snippet     logn
abbr        double logN(double logn)
options     head
  log(${1:n})/log(${2:base})

#ctype.h
snippet isalnum
abbr int isalnum(int c)
  isalnum(${1:c})

snippet     isalpha
abbr        int isalpha(int c)
  isalpha(${1:c})

snippet fabs
abbr double fabs(double n)
  fabs(${1:n})

#stdlib.h

snippet     malloc
abbr        void* malloc(size_t n_byte)
options     word
  malloc(${1:n_byte})

snippet     realloc
abbr        void* realloc(void* ptr,size_t n_byte)
options     word
  realloc(${1:ptr},${2:n_byte})

snippet     free
abbr        void free(void* ptr)
options     word
  free(${1:ptr})

snippet     srand
abbr        void srand(unsigned int seed)
options     word
  srand(${1:(unsigned)time(NULL)})

snippet     rand
abbr        int rand(void)
options     word
  rand()

